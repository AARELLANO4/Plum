
<div class = "productPage">
    <div class = "menu">
        
        <div>
            <h2 class = "menu1">Menu</h2><br>

            <ul>
                <li><a href="">New</a></li>
                <li><a href="">Discover</a></li>
                <li><a href="">Bestsellers</a></li>
                <li><a href="">Deals</a></li>
            </ul>
        </div>
        
        <div>
            <h3 class ="cat1">Categories</h3><br>

            <ul>
                <li><a href="">Books</a></li>
                <li><a href="">Video Games</a></li>
                <li><a href="">Music</a></li>
                <li><a href="">Movies</a></li>
                <li><a href="">TV Shows</a></li>
                <li><a href="">SmartHome</a></li>
                <li><a href="">Electronics</a></li>
                <li><a href="">Health and Beauty</a></li>
                <li><a href="">Stationery</a></li>
                <li><a href="">Home and Garden</a></li>
            </ul>
        </div>
    </div>

    <div class = "products">
        {{#each products}}
            <div class = "item">
                <img id = "productImage" src="{{this.image}}" alt="productImage">
                <p class = "prodTitle">{{this.title}}</p>
                <p class = "prodPrice">{{this.price}}</p>
                <p class = "prodCategory"><a href="">{{this.category}}</a></p>
            </div>
        {{/each}}
     </div>
    
</div>